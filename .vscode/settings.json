{
    "cSpell.words": [
        "Agrawal",
        "Bowlby's",
        "Chakrapani",
        "Handouzia",
        "Naseem",
        "Tarnowski"
    ],
    "cSpell.enableFiletypes": [
        "!python"
    ],
    "AutoPreview.serverURL": "localhost:3030",
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex ➜ bibtex ➜ makeglossaries ➜ pdflatex (x2)",
            "tools": [
                "pdflatex",
                "bibtex",
                "makeglossaries",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "xelatexmk -> bibtex -> makeglossaries ➜ xelatexmk * 2",
            "tools": [
                "xelatexmk",
                "bibtex",
                "makeglossaries",
                "xelatexmk",
                "xelatexmk"
            ]
        },
         {
            "name": "xelatexmk * 2",
            "tools": [
                "xelatexmk",
                "xelatexmk"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-shell-escape",
                "%DOC%"
            ]
        },
        {
            "name": "xelatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-shell-escape",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "makeglossaries",
            "command": "makeglossaries",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        },
    ],
    "workbench.colorTheme": "Monokai Pro"
}